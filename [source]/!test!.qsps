# !--test--!
if $user_text='logs':
	$func('em.arr.print','$log','[only result]')
elseif instr($lcase($user_text),'sqrt')=1:
	$args[0]=$trim($replace($replace($replace($user_text,'sqrt'),'('),')'))
	if isnum($args[0])=0: $args[0]=$dyneval("$result=$str(<<$args[0]>>)")
	$func('#sqrt#',$args[0])
elseif $user_text='gen id':
	*pl $func('em.str.random',8,'[ot]','0123456789abcdef')+'-'+$func('em.str.random',4,'[ot]','0123456789abcdef')+'-'+$func('em.str.random',4,'[ot]','0123456789abcdef')+'-'+$func('em.str.random',4,'[ot]','0123456789abcdef')+'-'+$func('em.str.random',12,'[ot]','0123456789abcdef')
elseif $user_text='gen pass':
	*pl $func('em.str.random',16,'[inc][\z][\d]','!@#$')
else
	dynamic $user_text
end
